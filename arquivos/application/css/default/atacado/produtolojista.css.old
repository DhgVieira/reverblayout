    float: left !important;
    width: 443px !important;
    height: 494px;
}

.image img {
    max-width: 443px !important;
    max-height: 494px;
}

.btn {
    display: table-cell;
    vertical-align: middle;
}

#prod-thumbnails-list {
    position: absolute;
    width: 60px;
}

#hide-thumbnails {
    position: relative;
    float: left;
    overflow: hidden;
    margin-top: 20px;
    margin-bottom: 20px;
    width: 65px;
    height: 447px;
}

#hide-thumbnails li {
    position: relative;
    float: left;
    margin: 0 0 2px 0;
    width: 100%;
    height: 70px;
}
#hide-thumbnails li a{
    display: block;
    float: left;
    width: 100%;
    height: 100%;
}
#hide-thumbnails li img{
    max-width: 100%;
    max-height: 100%;
}

.images {
    position: relative;
    float: left;
}

.images .thumbnails > .nav-pills-item {
    margin-bottom: 5px !important;
}

.nav-pills {
    width: 120px;
}

.nav-pills > .nav-pills-item > a {
    padding-left: 7px;
    color: #7c7c7c;
    text-transform: uppercase;
    min-height: 20px;
    line-height: 20px;
    width: 100%;
    display: block;
}
.nav-pills > .nav-pills-item:hover {
    color: #4E4D4D;
    background-color: #e7e7e7;
    font-weight: bold;
}

.nav-pills .nav-pills-item {
    display: block;
    clear: both;
    margin-bottom: 5px;
    font-size: 10px;
    position: relative;
}

/*submenu*/
.nav-pills > .nav-pills-item:hover > .nav-pills-sub{
    display: block;
}
.nav-pills-sub {
    display: none;
    width: 150px;
    position: absolute;
    left: 120px;
    top: 0;
    z-index: 20;
    background-color: #d2d2d2;
}

.nav-pills-sub .nav-pills-sub-item{
    display: block;
    width: 100%;
    height: 20px;
    line-height: 20px;
    background-color: #d2d2d2;
    padding-left: 20px;
    font-weight: normal;
}
.nav-pills-sub .nav-pills-sub-item:hover{
    background-color: #b2b2b2;
    font-weight: bold;
}
.nav-pills-sub .nav-pills-sub-item > a{
    color: #7c7c7c;
    text-transform: uppercase;
    display: block;
    width: 100%;
    height: 20px;
    line-height: 20px;
}

.row-fluid h2 {
    color: #e85238;
    text-transform: uppercase;
    font-size: 18px;
}

#uppers {
    margin: 10px 0px;
}

#prod-detalhe {
    margin-bottom: 20px;
}

.btn-block:hover {
    background-color: #e85238 !important;
}

.description .left {
    position: relative;
}

.troca {
    position: absolute;
    top: -66px;
    left: -35px;
    z-index: 999;
    display: none;
    padding-top: 8px;
    width: 202px;
    height: 120px;
    border-radius: 10px;
    background-color: #000;
    color: #fff;
    text-align: justify;
    font-size: 10px;
    line-height: 11px;
}

.troca:after {
    position: absolute;
    bottom: -5px;
    left: 50%;
    margin-left: -5px;
    width: 0;
    height: 0;
    border-top: 8px solid #000;
    border-right: 5px solid transparent;
    border-left: 5px solid transparent;
    content: " ";
}

.troca p {
    margin: 0px;
    padding: 0 7px;
    font-size: 10px;
}

