/* Coluna de listagem de produtos
========================================================================== */

@font-face {
    font-weight: normal;
    font-style: normal;
    font-family: 'dinnextltpro-bold';
    src: url('../../../../default/fonts/dinnextltpro-bold.eot');
    src: url('../../../../default/fonts/dinnextltpro-bold.eot?#iefix') format('embedded-opentype'),
    url('../../../../default/fonts/dinnextltpro-bold.woff') format('woff'),
    url('../../../../default/fonts/dinnextltpro-bold.ttf') format('truetype'),
    url('../../../../default/fonts/dinnextltpro-bold.svg#Bariol-Regular') format('svg');
}

.aviseme {
    width: 100%;
    text-transform: uppercase;
    font-size: 11px;
}

.product-thumb,
.product-thumb img {
    display: block;
}

.product-title {
    margin: 5px 0px;
    text-transform: uppercase;
    font-weight: bold;
    font-size: 10px;
}

.product-title a {
    color: #63be99;
}

.product-title a:hover,
.product-title a:focus,
.prices p {
    color: #414042;
}

.prices {
    clear: both;
}

.prices p {
    font-size: 10px;
}

.prices strong {
    font-weight: bold;
}

.ui-spinner {
    position: relative;
    clear: both;
    margin: 10px 0px;
    font-size: 10px;
}

.label-item {
    font-weight: bold;
}

/* Botão de alterar quantidade */
.quantity-spinner {
    padding: 3px 4px;
    width: 30px;
    border: 1px solid #c4c3c3;
    background-color: #fff;
    font-size: 10px;
}

.ui-spinner-button {
    position: absolute;
    right: 13px;
    width: 8px;
    height: 8px;
    background: url("../../../../default/images/icon/spinner-arrow.png") no-repeat;
    cursor: pointer;
}

.ui-spinner-up {
    top: 1px;
    background-position: 0px 0px;
}

.ui-spinner-up:hover,
.ui-spinner-up:focus {
    background-position: 0px -18px;
}

.ui-spinner-down {
    top: 14px;
    background-position: 0px -10px;
}

.ui-spinner-down:hover,
.ui-spinner-down:focus {
    background-position: 0px -28px;
}

/* Botão de adicionar ao carrinho */
.ui-buttons {
    position: absolute;
    bottom: 7px;
    clear: both;
    margin: 0 auto;
    width: 100%;
}

.ui-button {
    display: inline-block;
    padding: 4px 7px;
    border: 1px solid #61bf98;
    border-radius: 3px;
    background-color: #fff;
    color: #61bf98;
    text-transform: uppercase;
    font-weight: bold;
    font-size: 10px;
}

.ui-button:hover,
.ui-button:focus {
    border: 1px solid #414042;
    color: #414042;
}

.ui-button.add {
    margin-top: 38px;
}

.ui-gender-icon {
    margin-top: 10px;
    height: 27px;
    background: url("../../../../default/images/icon/genders.png") no-repeat;
}

.ui-gender-icon.female {
    margin-top: 5px;
    background-position: 18px -32px;
}

.ui-gender-icon.male {
    padding-left: 15px;
    background-position: 8px 0px;
}

.ui-gender-icon .ui-button {
    margin-top: 5px;
    padding: 2px 4px;
}

.btn-block {
    position: relative;
    float: left;
    margin-bottom: 6px;
    width: 100%;
    height: 27px;
    line-height: 27px;
    border-radius: 5px;
    background: #949697;
    color: #fff;
    text-align: center;
    text-transform: uppercase;
    font-size: 10px;
    font-family: sans-serif;
}

.title {
    padding-left: 10px !important;
    text-transform: uppercase;
    font-size: 12px;
}

.size-list {
    display: table;
}

.sizes .content {
    float: left;
    padding: 10px 0;
    height: auto !important;
}

#rating-star1 ~ .rating li {
    background-color: transparent !important;
}

#rating-star2 ~ .rating li {
    background-color: transparent !important;
}

#rating-star3 ~ .rating li {
    background-color: transparent !important;
}

#rating-star4 ~ .rating li {
    background-color: transparent !important;
}

#rating-star5 ~ .rating li {
    background-color: transparent !important;
}

#moveprev {
    position: absolute;
    top: 3px;
    left: 20px;
    clear: both;
    width: 18px;
    height: 13px;
    border: 0px;
    background: url(../../../../default/images/arrows-carousel.png) no-repeat top center;
    cursor: pointer;
}

.shifted-in {
    font-size: 0px;
}

#movenext {
    position: absolute;
    bottom: 3px;
    left: 20px;
    clear: both;
    width: 18px;
    height: 13px;
    border: 0px;
    background: url(../../../../default/images/arrows-carousel.png) no-repeat bottom center;
